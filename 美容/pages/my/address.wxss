/* pages/my/address.wxss */
page{
    background-color: #f1f0f6;
}
.ts{
    font-size: 32rpx;
    color: #919193;
    text-align: center;
    margin-top: 60rpx;
}
.get{
    width: 100%;
    justify-content: space-around;
    display: flex;
    margin-top: 60rpx;
}
.get>view{
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    width: 328rpx;
    height: 74rpx;
    border-radius: 10rpx;
    line-height: 74rpx;
}
.get1{
    background-color: #fd5143;
}
.get2{
    background-color: #f39704;
}
.lists>view{
    width: 720rpx;
    margin: 0 auto;
    background-color: #fff;
    border-radius: 20rpx;
    margin-top: 20rpx;
}
.bj{
    width: 50rpx;
    height: 50rpx;
    padding: 10rpx;
    flex-shrink: 0;
}
.listTop{
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #eee;
    padding: 30rpx 40rpx;
    box-sizing: border-box ;
}
.name{
    font-size: 32rpx;
    margin-bottom: 20rpx;
}
.name text{
    color: #959394;
    margin-left: 10rpx;
}
.address,.detailed{
    font-size: 30rpx;
    line-height: 50rpx;
}
.detailed{
    color: #959394;
    width: 95%;
}
.address text{
    color: #ff5345;
    margin-right: 10rpx;
}
.listDown{
    color: #959394;
    line-height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    font-size: 28rpx;
    padding: 0 30rpx;
}
.listDown image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}
.click{
    display: flex;
    align-items: center;
}